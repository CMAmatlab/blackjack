function[newhand]=CardName(hand)
%single input, single output

newhand=strings(size(hand)); %Define a new string
for ii=1:length(newhand) %run a loop to rename face cards, tens, and aces
    if hand(ii)== 1
        newhand(ii)= 'A';
    elseif hand(ii)==10
        newhand(ii)='T';
    elseif hand(ii)==11
        newhand(ii)='J';
    elseif hand(ii)==12
        newhand(ii)='Q';
    elseif hand(ii)==13
        newhand(ii)='K';
    else
        newhand(ii)=hand(ii);
    end
end
end
