function[x]=CardName(n)
x=strings(size(n));
for ii=1:length(n)
    if n(ii)== 1
        x(ii)= 'A';
    elseif n(ii)==10
        x(ii)='T';
    elseif n(ii)==11
        x(ii)='J';
    elseif n(ii)==12
        x(ii)='Q';
    elseif n(ii)==13
        x(ii)='K';
    else
        x(ii)=n(ii);
    end
end
end
